model{
  for(i in 1:I) {
  
    Y[i, 1:J[i]] ~ dmulti(p[1:J[i]], N[i])
    
    N[i] ~ dpois(NN[i] * sum(p[1:J[i] ]))
  
    
  }
    
    for (j in 2:Jmax) {
      p[j] <- pweib(j, s, r) -  pweib(j, s, r)
    }
    p[1] <- pweib(1, s, r)

########## PRIORS:
  

  
  r ~ dgamma(1.0E-2, 1.0E-2)
  s ~ dgamma(1.0E-2, 1.0E-2)


  for (i in 1:I) {
    NN[i] ~  dgamma(1.0E-3, 1.0E-3)
  }

}